server {
    listen 80;
    server_name web.your-domain.com;

    root /var/www/readandlead/build;
    index index.html;

    location / {
        try_files $uri /index.html;
        expires 1h;
    }

    # Optionally proxy API calls to your API domain
    # location /api/ {
    #     proxy_pass https://api.your-domain.com;
    # }
}

